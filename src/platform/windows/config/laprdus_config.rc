// -*- coding: utf-8 -*-
// laprdus_config.rc - LaprdusTTS configuration GUI resources

#include <windows.h>
#include <commctrl.h>
#include "resource.h"

// Tell the resource compiler this file is UTF-8 encoded
#pragma code_page(65001)

// Application manifest for visual styles and DPI awareness
1 RT_MANIFEST "laprdus_config.manifest"

// =============================================================================
// Dialog Template
// =============================================================================

IDD_CONFIG_DIALOG DIALOGEX 0, 0, 320, 385
STYLE DS_SETFONT | DS_MODALFRAME | DS_FIXEDSYS | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "LaprdusTTS Settings"
FONT 8, "MS Shell Dlg", 0, 0, 0x1
BEGIN
    // Voice Settings Group
    GROUPBOX        "", IDC_GROUP_VOICE, 7, 7, 306, 145

    LTEXT           "Voice:", IDC_VOICE_LABEL, 14, 22, 50, 8
    COMBOBOX        IDC_VOICE_COMBO, 70, 20, 180, 100, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP

    LTEXT           "Speed:", IDC_SPEED_LABEL, 14, 42, 50, 8
    CONTROL         "", IDC_SPEED_SLIDER, TRACKBAR_CLASS, TBS_HORZ | TBS_AUTOTICKS | WS_TABSTOP, 70, 40, 180, 18
    LTEXT           "1.0x", IDC_SPEED_VALUE, 260, 42, 40, 8
    AUTOCHECKBOX    "", IDC_FORCE_SPEED, 14, 58, 290, 10, WS_TABSTOP

    LTEXT           "Pitch:", IDC_PITCH_LABEL, 14, 75, 50, 8
    CONTROL         "", IDC_PITCH_SLIDER, TRACKBAR_CLASS, TBS_HORZ | TBS_AUTOTICKS | WS_TABSTOP, 70, 73, 180, 18
    LTEXT           "1.0x", IDC_PITCH_VALUE, 260, 75, 40, 8
    AUTOCHECKBOX    "", IDC_FORCE_PITCH, 14, 91, 290, 10, WS_TABSTOP

    LTEXT           "Volume:", IDC_VOLUME_LABEL, 14, 108, 50, 8
    CONTROL         "", IDC_VOLUME_SLIDER, TRACKBAR_CLASS, TBS_HORZ | TBS_AUTOTICKS | WS_TABSTOP, 70, 106, 180, 18
    LTEXT           "100%", IDC_VOLUME_VALUE, 260, 108, 40, 8
    AUTOCHECKBOX    "", IDC_FORCE_VOLUME, 14, 124, 290, 10, WS_TABSTOP

    // Pause Settings Group
    GROUPBOX        "", IDC_GROUP_PAUSES, 7, 157, 306, 90

    LTEXT           "Comma pause:", IDC_COMMA_LABEL, 14, 172, 80, 8
    CONTROL         "", IDC_COMMA_SLIDER, TRACKBAR_CLASS, TBS_HORZ | TBS_AUTOTICKS | WS_TABSTOP, 100, 170, 150, 18
    LTEXT           "100 ms", IDC_COMMA_VALUE, 260, 172, 40, 8

    LTEXT           "Sentence pause:", IDC_SENTENCE_LABEL, 14, 195, 80, 8
    CONTROL         "", IDC_SENTENCE_SLIDER, TRACKBAR_CLASS, TBS_HORZ | TBS_AUTOTICKS | WS_TABSTOP, 100, 193, 150, 18
    LTEXT           "100 ms", IDC_SENTENCE_VALUE, 260, 195, 40, 8

    LTEXT           "Newline pause:", IDC_NEWLINE_LABEL, 14, 218, 80, 8
    CONTROL         "", IDC_NEWLINE_SLIDER, TRACKBAR_CLASS, TBS_HORZ | TBS_AUTOTICKS | WS_TABSTOP, 100, 216, 150, 18
    LTEXT           "100 ms", IDC_NEWLINE_VALUE, 260, 218, 40, 8

    // Options Group
    GROUPBOX        "", IDC_GROUP_OPTIONS, 7, 252, 306, 55

    AUTOCHECKBOX    "", IDC_INFLECTION_CHECK, 14, 267, 290, 10, WS_TABSTOP
    AUTOCHECKBOX    "", IDC_EMOJI_CHECK, 14, 282, 290, 10, WS_TABSTOP
    AUTOCHECKBOX    "", IDC_DIGITS_CHECK, 14, 297, 290, 10, WS_TABSTOP

    // Dictionaries Group
    GROUPBOX        "", IDC_GROUP_DICTIONARIES, 7, 312, 306, 40

    AUTOCHECKBOX    "", IDC_USER_DICT_CHECK, 14, 327, 170, 10, WS_TABSTOP
    PUSHBUTTON      "", IDC_DICT_BUTTON, 213, 325, 90, 14, WS_TABSTOP

    // Buttons
    PUSHBUTTON      "Test", IDC_TEST_BUTTON, 7, 360, 60, 14, WS_TABSTOP
    DEFPUSHBUTTON   "OK", IDOK, 140, 360, 50, 14, WS_TABSTOP
    PUSHBUTTON      "Cancel", IDCANCEL, 196, 360, 50, 14, WS_TABSTOP
    PUSHBUTTON      "Apply", IDC_APPLY_BUTTON, 252, 360, 50, 14, WS_TABSTOP
END

// =============================================================================
// Dictionary Management Dialog
// =============================================================================

IDD_DICTIONARY_DIALOG DIALOGEX 0, 0, 400, 320
STYLE DS_SETFONT | DS_MODALFRAME | DS_FIXEDSYS | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Rječnici"
FONT 8, "MS Shell Dlg", 0, 0, 0x1
BEGIN
    LTEXT           "", IDC_DICT_TYPE_LABEL, 7, 10, 80, 8
    COMBOBOX        IDC_DICT_TYPE_COMBO, 90, 8, 180, 100, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    CONTROL         "", IDC_DICT_LISTVIEW, WC_LISTVIEWW, LVS_REPORT | LVS_SINGLESEL | LVS_SHOWSELALWAYS | WS_BORDER | WS_TABSTOP, 7, 28, 386, 252
    PUSHBUTTON      "", IDC_DICT_ADD, 7, 290, 60, 14, WS_TABSTOP
    PUSHBUTTON      "", IDC_DICT_EDIT, 73, 290, 60, 14, WS_TABSTOP
    PUSHBUTTON      "", IDC_DICT_DUPLICATE, 139, 290, 70, 14, WS_TABSTOP
    PUSHBUTTON      "", IDC_DICT_DELETE, 215, 290, 60, 14, WS_TABSTOP
    PUSHBUTTON      "", IDC_DICT_CLOSE, 333, 290, 60, 14, WS_TABSTOP
END

// =============================================================================
// Dictionary Entry Editor Dialog
// =============================================================================

IDD_DICT_ENTRY_DIALOG DIALOGEX 0, 0, 300, 190
STYLE DS_SETFONT | DS_MODALFRAME | DS_FIXEDSYS | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Dodaj unos"
FONT 8, "MS Shell Dlg", 0, 0, 0x1
BEGIN
    LTEXT           "", IDC_ENTRY_ORIGINAL_LABEL, 7, 10, 100, 8
    EDITTEXT        IDC_ENTRY_ORIGINAL_EDIT, 7, 22, 286, 14, ES_AUTOHSCROLL
    LTEXT           "", IDC_ENTRY_REPLACEMENT_LABEL, 7, 42, 100, 8
    EDITTEXT        IDC_ENTRY_REPLACEMENT_EDIT, 7, 54, 286, 14, ES_AUTOHSCROLL
    LTEXT           "", IDC_ENTRY_COMMENT_LABEL, 7, 74, 250, 8
    EDITTEXT        IDC_ENTRY_COMMENT_EDIT, 7, 86, 286, 14, ES_AUTOHSCROLL
    AUTOCHECKBOX    "", IDC_ENTRY_CASE_SENSITIVE, 7, 110, 200, 10, WS_TABSTOP
    AUTOCHECKBOX    "", IDC_ENTRY_WHOLE_WORD, 7, 125, 200, 10, WS_TABSTOP
    DEFPUSHBUTTON   "U redu", IDOK, 186, 165, 50, 14, WS_TABSTOP
    PUSHBUTTON      "Odustani", IDCANCEL, 243, 165, 50, 14, WS_TABSTOP
END

